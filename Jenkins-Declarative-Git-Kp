pipeline{
    agent any
    environment{
        JAVA_HOME = "/usr/lib/jvm/java-11-openjdk-amd64"
        PATH = "/opt/maven/bin:$PATH"
        export PATH=$PATH:$JAVA_HOME/bin
    }
    stages{
        stage("Git Checkout"){
            steps{
            git credentialsId: 'github', url: 'https://github.com/kpreddy87/myweb'
            }
        }
         stage("Maven Build"){
            steps{
                sh "mvn clean package"
               
            }
        }
    }
}
