pipeline{
    agent any
    environment{
        PATH = "/opt/maven/bin:$PATH"
        PATH = "/usr/lib/jvm/java-11-openjdk-amd64/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
            git credentialsId: 'github', url: 'https://github.com/kpreddy87/myweb'
            }
        }
         stage("Maven Build"){
            steps{
                sh "mvn clean package"
               
            }
        }
    }
}
